{
  "name": "dudhiyadairy",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.8.0",
    "node-sass-chokidar": "^1.3.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "devDependencies": {
    "babel-plugin-import": "^1.8.0",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "build-css": "node-sass-chokidar ./src/assets/sass/main.scss -o ./src/assets/css",
    "watch-css": "npm run build-css && node-sass-chokidar ./src/assets/sass/main.scss -o ./src/assets/css --watch --recursive",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
